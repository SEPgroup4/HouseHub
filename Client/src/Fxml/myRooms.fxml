<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" stylesheets="@FilesCSS/manage.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyRoomsViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="482.0" spacing="10.0">
         <children>
            <Button graphicTextGap="10" onAction="#goBack" prefHeight="26.0" prefWidth="22.0">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="25.0">
                     <image>
                        <Image url="@../Resources/GoBack.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label text="My Rooms" />
         </children>
      </HBox>
      <VBox prefHeight="361.0" prefWidth="579.0">
         <children>
            <TableView fx:id="roomsTable" prefHeight="283.0" prefWidth="579.0">
              <columns>
                <TableColumn fx:id="roomId" prefWidth="107.99999845027924" text="Room Id" />
                <TableColumn fx:id="bedrooms" prefWidth="116.00000762939453" text="Bedrooms" />
                  <TableColumn fx:id="price" minWidth="0.0" prefWidth="68.79998779296875" text="Price" />
                  <TableColumn fx:id="address" prefWidth="162.39996337890625" text="Address" />
                  <TableColumn fx:id="reserved" prefWidth="124.79995727539062" text="Reserved" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#remove" styleClass="action-button" text="Remove">
               <HBox.margin>
                  <Insets left="250.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
